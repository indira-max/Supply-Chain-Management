<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active_Supplier</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Active__c = True,
        ISBLANK( Contract_End_Date__c ) )</errorConditionFormula>
    <errorMessage>Active suppliers must have a contract end date.</errorMessage>
</ValidationRule>
