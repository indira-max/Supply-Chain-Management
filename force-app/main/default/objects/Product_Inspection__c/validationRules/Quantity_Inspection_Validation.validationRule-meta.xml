<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantity_Inspection_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Quantity_Inspected__c &lt;= 0, ISBLANK( Quantity_Inspected__c ) )</errorConditionFormula>
    <errorMessage>Quantity Inspected must be greater than zero</errorMessage>
</ValidationRule>
